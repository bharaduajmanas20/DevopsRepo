        <fullName>Status__c</fullName>
        <externalId>false</externalId>
        <label>Status</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Ordered</fullName>
                    <default>false</default>
                    <label>Ordered</label>
                </value>
                <value>
                    <fullName>Dispatched</fullName>
                    <default>false</default>
                    <label>Dispatched</label>
                </value>
                <value>
                    <fullName>Delivered</fullName>
                    <default>false</default>
                    <label>Delivered</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Total_Price__c</fullName>
        <externalId>false</externalId>
        <formula>IF(Product__r.Cost_of_Products__c  *  Quantity__c * 1.18&gt;500, Product__r.Cost_of_Products__c  *  Quantity__c * 1.18, Product__r.Cost_of_Products__c  *  Quantity__c * 1.18+90 )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <label>Order</label>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
