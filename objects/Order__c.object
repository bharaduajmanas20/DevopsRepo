    <validationRules>
        <fullName>Delivered_Status_Cannot_be_Changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(  Status__c  ),
    OR(
       ISPICKVAL(PRIORVALUE( Status__c ),&quot;Delivered&quot;),
       ISPICKVAL(PRIORVALUE( Status__c ),&quot;Delivered&quot;)
     )
)</errorConditionFormula>
        <errorMessage>You Can&apos;t Change the Order status now</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_past_date</fullName>
        <active>true</active>
        <errorConditionFormula>Date_of_Order__c   &lt; TODAY()</errorConditionFormula>
        <errorMessage>Phone Field Can&apos;t be empty</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Phone_Field</fullName>
        <active>false</active>
        <errorConditionFormula>NOT(REGEX(Phone_Number__c, &quot;[7-9]{1}[0-9]{9}&quot;))</errorConditionFormula>
        <errorMessage>Must Contain 10 Digits Only</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Total_Price_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Total_Price__c&gt;2000, ISNULL(GST_INN__c))</errorConditionFormula>
        <errorMessage>More than 2000</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
